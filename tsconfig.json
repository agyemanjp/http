{
	"compilerOptions": {
		"target": "es2015",
		"module": "es6",
		"moduleResolution": "node",
		"traceResolution": false,
		"declaration": true,
		"lib": [
			"es7",
			"es6",
			"es5",
			"ES2018",
			"dom",
			"es2015.promise",
			"scripthost",
			"dom.iterable",
			"esnext.asynciterable"
		],
		"strict": true,
		"keyofStringsOnly": true,
		"strictNullChecks": true,
		"skipLibCheck": true,
		"incremental": true,
		"rootDirs": [
			"./src",
			"./src/functional",
			"./src/collections",
			"./src/datetime",
			"./src/stats",
			"./src/async",
			"./src/object",
			"./src/numeric",
			"./src/utility",
			"./src/text",
		],
		"outDir": "./dist",
		"sourceMap": true
	},
	"include": [
		"./src",
	],
	"exclude": []
}